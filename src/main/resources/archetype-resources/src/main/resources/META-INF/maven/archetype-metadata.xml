<?xml version="1.0" encoding="utf-8" ?>
<archetype-descriptor>
    <fileSets>
        <fileSet filtered="true" packaged="true">
            <directory>src/main/java</directory>
            <includes>
                <include>**/*</include>
            </includes>
            <excludes>
                <exclude>**/.gitkeep</exclude>
            </excludes>
        </fileSet>
        <fileSet filtered="true">
            <directory></directory>
            <includes>
                <include>**/*</include>
            </includes>
            <excludes>
                <exclude>src/main/java/**/*</exclude>
                <exclude>**/.gitkeep</exclude>
            </excludes>
        </fileSet>
    </fileSets>
